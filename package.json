{
  "name": "@danskify/dictionary",
  "version": "2.0.0",
  "description": "A Danish dictionary data package with 20k entries",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://en.wiktionary.org/wiki/User:Matthias_Buchmeier",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json && cp ./src/data-info.txt ./dist/data-info.txt",
    "prepublishOnly": "cp ./data/data-clean.json ./src/data.json && yarn build",
    "generate": "ts-node ./scripts/parser.ts && yarn analyze:see-rejections",
    "generate:production": "VALIDATE_SEMANTICS=true yarn generate",
    "generate:validation": "ts-node ./scripts/generate-validation-sample.ts",
    "analyze:see-rejections": "ts-node ./scripts/analyze-see.ts",
    "remove-offensive": "ts-node ./scripts/filter-offensive.ts",
    "validate:sample": "ts-node ./scripts/validate-sample.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "danish",
    "dictionary",
    "danskify"
  ],
  "author": "zoeyfrisart",
  "license": "CC-BY-SA-3.0",
  "contributors": [
    {
      "name": "Wiktionary contributors (Matthias Buchmeier et al.)",
      "url": "https://en.wiktionary.org/wiki/User:Matthias_Buchmeier"
    }
  ],
  "packageManager": "yarn@4.10.3",
  "devDependencies": {
    "@eslint/compat": "1.2.3",
    "@eslint/js": "9.16.0",
    "@types/eslint__js": "8.42.3",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "8.17.0",
    "@typescript-eslint/parser": "8.17.0",
    "@xenova/transformers": "^2.17.2",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "eslint": "9.16.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import-x": "4.5.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-perfectionist": "3.9.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-react-refresh": "0.4.16",
    "eslint-plugin-unicorn": "56.0.1",
    "globals": "15.13.0",
    "openai": "^6.8.1",
    "prettier": "3.4.2",
    "prettier-eslint": "16.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "8.17.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoeyfrisart/danskify-dictionary.git"
  },
  "bugs": {
    "url": "https://github.com/zoeyfrisart/danskify-dictionary/issues"
  }
}
